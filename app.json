    "slug": "clyp",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/ico.png",
    "splash": {
      "image": "./assets/spl.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
