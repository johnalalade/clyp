    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.clyp.app"
    },
    "android": {
      "package": "com.clyp.app",
      "versionCode": 2
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
